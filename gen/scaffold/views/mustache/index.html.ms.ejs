<div class="jumbotron">
  <h2>All <%= names.constructor.plural %></h2>
  <a href="/<%= names.property.plural %>/add" class="btn btn-default pull-right">Create a new <%= names.constructor.singular %></a>
</div>

<div id="<%= names.property.plural %>-list">
{{#<%= names.property.plural %>}}
  <div class="row list-item" id="<%= names.property.singular %>-{{id}}">
    <div class="col-md-6">
      <h3><a href="/<%= names.property.plural %>/{{id}}">{{id}}</a></h3>
    </div>
    <div class="col-md-6">
      <h3><i class="glyphicon-list-alt"></i>{{id}}</h3>
    </div>
  </div>
{{/<%= names.property.plural %>}}
</div>
